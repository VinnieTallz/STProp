# Seam.co Configuration
# Copy this file to .env and fill in your actual values

# Get these from your Seam Console: https://console.seam.co/
VITE_SEAM_API_KEY=seam_apikey_your_key_here
VITE_SEAM_WORKSPACE_ID=your_workspace_id_here

# Environment: 'sandbox' for testing, 'production' for live locks
VITE_SEAM_ENVIRONMENT=sandbox

# Optional: Custom API endpoint (leave blank for default)
VITE_SEAM_API_ENDPOINT=

# Security Settings
VITE_ENABLE_AUDIT_LOGGING=true
VITE_MAX_API_REQUESTS_PER_MINUTE=60